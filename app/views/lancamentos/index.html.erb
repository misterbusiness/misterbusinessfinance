            <li class="">&nbsp;</li>
            <li class="">&nbsp;</li>
            <li class="">&nbsp;</li>
          </ul>
        </div>
      </div>
    </div>
    <div class="col-lg-11">
      <div class="row">
        <div class="col-lg-5">
          <div class="panel panel-default form">
            <div class="panel-body">
              <%= render :partial => 'form' %>
            </div>
          </div>
        </div>
        <div class="col-lg-3">
          <%= render :partial => 'notifications' %>
        </div>
        <div class="col-lg-4">
