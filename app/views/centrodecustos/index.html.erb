<h1>Listing centrodecustos</h1>

<table>
  <tr>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @centrodecustos.each do |centrodecusto| %>
  <tr>
    <td><%= centrodecusto.descricao %></td>
    <td><%= link_to 'Show', centrodecusto %></td>
    <td><%= link_to 'Edit', edit_centrodecusto_path(centrodecusto) %></td>
    <td><%= link_to 'Destroy', centrodecusto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
